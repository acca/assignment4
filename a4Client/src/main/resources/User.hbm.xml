<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="it.unitn.dsantoro.a4client.User" optimistic-lock="version" polymorphism="implicit" select-before-update="false" table="USERS">
    <id column="USER_IDOP" name="idOp">
      <generator class="increment"/>
    </id>
    <property column="USER_MONEY" name="money" type="float"/>
    <property column="USER_STOCKS" name="stocksAmount" type="int"/>
    <property column="USER_ID" name="id" type="long"/>
  </class>
</hibernate-mapping>
